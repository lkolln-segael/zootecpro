@model IEnumerable<WebZootecPro.ViewModels.Inseminador.InseminadorViewModel>

  <h2>Inseminadores</h2>

  <p>
    <a asp-action="Create" class="btn btn-primary">Nuevo</a>
  </p>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in Model)
      {
      <tr>
        <td>@item.Nombre</td>
        <td>@item.Apellido</td>
        <td>
          <a asp-action="Details" asp-route-id="@item.Id">Ver</a> |
          <a asp-action="Edit" asp-route-id="@item.Id">Editar</a> |
          <a asp-action="Delete" asp-route-id="@item.Id">Eliminar</a>
        </td>
      </tr>
      }
    </tbody>
  </table>
