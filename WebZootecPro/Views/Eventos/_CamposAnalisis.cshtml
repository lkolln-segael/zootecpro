@model WebZootecPro.ViewModels.Eventos.RegistrarEventoViewModel

<div class="row g-3">
    <div class="col-lg-6 col-md-12">
        <label asp-for="TipoAnalisis" class="form-label"></label>
        <input asp-for="TipoAnalisis" class="form-control" placeholder="Ej: Brucelosis, Mastitis, etc." />
        <span asp-validation-for="TipoAnalisis" class="text-danger small"></span>
    </div>
    <div class="col-lg-6 col-md-12">
        <label asp-for="Resultado" class="form-label"></label>
        <input asp-for="Resultado" class="form-control" placeholder="Ej: Positivo / Negativo" />
        <span asp-validation-for="Resultado" class="text-danger small"></span>
    </div>
</div>
